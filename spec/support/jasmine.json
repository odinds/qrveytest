{
  "spec_dir": "spec/",
  "spec_files": [
    "**/*[sS]pec.ts"
  ],
  "stopSpecOnExpectationFailure": false,
  "random": true,
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "coveragePreprocessor":{
    "exclude": [ "**/*[sS]pec.ts", "**/spec/**/*" ]
  }    
}
